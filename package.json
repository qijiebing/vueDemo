{
  "name": "tt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "npm run compile",
    "build-stage": "npm run compile",
    "build-qa": "npm run compile",
    "build-product": "npm run compile",
    "compile": "rm -rf ./dist && ./node_modules/.bin/vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "stop": "pm2 stop all",
    "dev": "pm2 start ./server/server.js -i 2 --watch && npm run serve",
    "server": "./node_modules/.bin/pm2-runtime ./server/server.js -i 2",
    "start": "./node_modules/.bin/pm2-runtime ./server/server.js  -i 2",
    "deploy": "node ./deploy/index.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "fastclick": "^1.0.6",
    "jsbarcode": "^3.11.0",
    "jsonp": "^0.2.1",
    "koa-compress": "^3.0.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa2": "^2.0.0-alpha.7",
    "mand-mobile": "^2.3.2",
    "minirefresh": "^2.0.2",
    "pm2": "^5.2.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-i18n": "^8.11.2",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "postcss-px2rem": "^0.3.0",
    "vue-template-compiler": "^2.6.10"
  }
}
